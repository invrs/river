{
  "name": "starter-tasks",
  "version": "0.1.0",
  "description":
    "River tasks to start new projects and update old ones",
  "keywords": [
    "boilerplate",
    "starter",
    "task-env",
    "tasks"
  ],
  "author": "Inverse Labs <labs@inverse.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/invrs/river.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "homepage":
    "https://github.com/invrs/river/tree/master/packages/starter-tasks#readme",
  "scripts": {
    "build":
      "rimraf dist; babel lib --out-dir dist --source-maps",
    "fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "pretty": "prettier --write **/*.css **/*.json **/*.md",
    "lint": "eslint --ignore-path .gitignore --ext=js .",
    "test": "jest test"
  },
  "devDependencies": {
    "babel-cli": "6",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-async-to-generator": "6",
    "babel-plugin-transform-object-rest-spread": "6",
    "babel-preset-node6": "11",
    "commandland": "0.1",
    "fxtr": "1",
    "jest": "22",
    "rimraf": "^2.6.2"
  },
  "jest": {
    "setupFiles": ["./test/setup.js"],
    "testMatch": ["**/*Test.js"]
  },
  "dependencies": {
    "fs-extra": "^5.0.0"
  }
}
