{
  "watchman": {
    "triggers": [
      {
        "name": "sass",
        "expression": [
          "anyof",
          [
            "match",
            "lib/**/*.scss",
            "wholename"
          ]
        ],
        "command": [
          "npm",
          "run",
          "build"
        ]
      }
    ]
  }
}
